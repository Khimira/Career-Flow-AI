    {
      "parameters": {
        "jsCode": "// ================================================================\n// DEDUPLICATION LOGIC VIA GOOGLE SHEETS\n// ================================================================\n\n// 1. Capture all items coming from the \"Merge\" node\nconst allItems = $input.all();\n\n// 2. Separate entries based on data structure:\n// - RSS items have the \"link\" field (lowercase) and \"guid\"\n// - Google Sheets items have the \"Link\" field (Capitalized - per your Append node)\nconst sheetsHistory = [];\nconst newRSSJobs = [];\n\nallItems.forEach(item => {\n  const json = item.json;\n  \n  // If it has the \"Link\" column (from Sheets) and no \"guid\" (from RSS), it is history\n  if (json.Link && !json.guid) {\n    sheetsHistory.push(json);\n  } \n  // If it has \"link\" and \"guid\", it is a new job from the RSS/Scraper\n  else if (json.link) {\n    newRSSJobs.push(json);\n  }\n});\n\n// 3. Create a \"Set\" with historical links for O(1) fast lookup\n// Normalize strings by trimming to avoid duplicates due to formatting\nconst processedLinks = new Set(\n  sheetsHistory.map(row => {\n    return row.Link ? row.Link.trim() : \"\";\n  })\n);\n\n// 4. Helper function to clean URLs (removes tracking parameters if necessary)\nconst cleanUrl = (url) => {\n  if (!url) return \"\";\n  try {\n    const u = new URL(url);\n    // Returns the base URL. For RSS, the full link is usually the unique identifier.\n    return url.trim(); \n  } catch (e) {\n    return url;\n  }\n};\n\n// 5. Filter unique RSS jobs\nconst uniqueJobs = newRSSJobs.filter(job => {\n  const jobLink = cleanUrl(job.link);\n  \n  // THE MAGIC: If the link is NOT in the historical Set, it is a new job\n  const alreadyExists = processedLinks.has(jobLink);\n  \n  // Debug: Log ignored duplicate jobs if needed\n  // if (alreadyExists) console.log(`Ignoring duplicate job: ${job.title}`);\n  \n  return !alreadyExists;\n});\n\n\n// 6. Add useful metadata (job source) and return\nreturn uniqueJobs.map(job => {\n  let source = \"Other\";\n  const url = job.link || \"\";\n  \n  if (url.includes(\"weworkremotely\")) source = \"WWR\";\n  else if (url.includes(\"remotive\")) source = \"Remotive\";\n  else if (url.includes(\"remoteok\")) source = \"RemoteOK\";\n  else if (url.includes(\"linkedin\")) source = \"LinkedIn\";\n\n  return {\n    ...job,\n    job_source: source,\n    // Ensures the \"Link\" field (Capitalized) exists for future Google Sheets Append\n    Link: url \n  };\n});"
      },
