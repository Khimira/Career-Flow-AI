    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst sshOutput = items[0].json;\nlet rawText = sshOutput.stdout || sshOutput.text || \"\";\n\n// Cleanup to isolate JSON content between curly braces { }\nconst cleanText = rawText.replace(/^[^{]*/, '').replace(/[^}]*$/, '').trim();\n\ntry {\n  const jsonData = JSON.parse(cleanText);\n\n  return {\n    match: jsonData.match === true,\n    detected_level: jsonData.detected_level || '',\n    contact_email: jsonData.contact_email || 'Not Found',\n    summary: jsonData.summary || '',\n    justification: jsonData.justification || 'No justification provided.',\n    \n    // Injecting job data to prevent \"Paired Item\" errors in downstream nodes (e.g., Google Sheets)\n    // Note: Ensure the node name 'JS: Smart Deduplication' matches your actual node name\n    job_title: $('JS: Smart Deduplication').item.json.title,\n    job_link: $('JS: Smart Deduplication').item.json.Link,\n    \n    // Re-adding the debug object to ensure conditional nodes (If) don't fail\n    _debug: { \n      status: (sshOutput.code !== undefined) ? sshOutput.code : 0 \n    }\n  };\n} catch (e) {\n  // If parsing fails, send status 1 to steer the flow toward the error/log path\n  return { \n    match: false, \n    job_title: $('JS: Smart Deduplication').item.json.title,\n    job_link: $('JS: Smart Deduplication').item.json.Link,\n    summary: \"Processing error\", \n    justificative: \"Failed to parse AI JSON output.\",\n    _debug: { status: 1 } \n  };\n}"
      },
