    {
      "parameters": {
        "jsCode": "// ================================================================\n// SSH COVER LETTER PROCESSING LOGIC\n// Node: Code (after the Cover Letter SSH node)\n// ================================================================\n\n// Capture the output from the SSH execution\nconst sshOutput = $input.all()[0].json;\n\n// SSH usually returns an array; we target the first element\nconst firstResult = Array.isArray(sshOutput) ? sshOutput[0] : sshOutput;\n\n// Extract stdout (where the Gemini response is located)\nlet rawText = '';\n\nif (firstResult.stdout) {\n  rawText = firstResult.stdout;\n} else if (firstResult.text) {\n  rawText = firstResult.text;\n} else {\n  rawText = JSON.stringify(firstResult);\n}\n\n// Validate if any text was actually captured\nif (!rawText || rawText.trim() === '') {\n  return {\n    error: \"No output received from Gemini CLI for cover letter\",\n    debug_data: firstResult,\n    code: firstResult.code || 'unknown',\n    stderr: firstResult.stderr || ''\n  };\n}\n\n// Sanitize the string by removing Markdown code blocks (```json and ```)\nconst cleanText = rawText\n  .replace(/```json\\s*/g, '')\n  .replace(/```\\s*/g, '')\n  .trim();\n\n// Attempt to parse the sanitized JSON\ntry {\n  const coverLetter = JSON.parse(cleanText);\n  \n  // Validate presence of mandatory fields\n  if (!coverLetter.subject || !coverLetter.body_html) {\n    return {\n      error: \"Valid JSON found but missing required fields: 'subject' or 'body_html'\",\n      parsed_data: coverLetter,\n      raw_text: rawText\n    };\n  }\n  \n  // Return processed data formatted for the \"Wait/Approval\" node\n  return {\n    subject: coverLetter.subject,\n    body_html: coverLetter.body_html,\n    // Optional debug metadata\n    _debug: {\n      ssh_code: firstResult.code,\n      has_stderr: !!firstResult.stderr,\n      text_length: coverLetter.body_html.length\n    }\n  };\n  \n} catch (parseError) {\n  // If parsing fails, return structured error details\n  return {\n    error: \"Failed to parse cover letter JSON\",\n    parse_error: parseError.message,\n    raw_text: rawText,\n    clean_text: cleanText,\n    ssh_code: firstResult.code,\n    ssh_stderr: firstResult.stderr\n  };\n}"
      },
